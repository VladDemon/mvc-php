{% extends './layout/base.html.twig' %}


{% block content %}
<div class="container d-flex flex-column align-items-center mt-5">
    <h1 class="text-center mb-4">{{ title }}</h1>
    {% for elem in data %}
        <div class="card mb-3 text-center w-50 shadow">
            <div class="card-body">
                <h5 class="card-title">ID: {{ elem["id"] }}</h5>
                <h6 class="card-subtitle mb-2 text-muted"><strong>name</strong> --> {{ elem["name"] }}</h6>
            </div>
        </div>
    {% endfor %}
</div>
{% endblock %}